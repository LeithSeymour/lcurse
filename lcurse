#!/usr/bin/env python
# -*- coding: utf-8 -*-

import sys

sys.path.insert(0, "./modules")

from modules import application
from PyQt4 import Qt
 
if __name__ == "__main__":
	app = application.Application(sys.argv)
	Qt.QCoreApplication.setApplicationName("lcurse2")
	sys.exit(app.start())
sys.exit(42)
